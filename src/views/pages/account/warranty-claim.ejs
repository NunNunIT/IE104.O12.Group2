<!DOCTYPE html>
<html lang="vi">

<head>
	<meta charset="UTF-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,300,0..1,0" />

	<link rel="stylesheet" href="/styles/pages/account/warranty-claim.css" />

	<title>TECHTWO.</title>
</head>

<body>
	<%- include('../../partials/header', { header: header }) %>
	<%- include('../../partials/breadcrumb', { breadcrumbs: [ { text: 'Trang cá nhân', link: '/account/information' }, { text: 'Chính sách bảo hành', link:'' } ] }) %>

	<main class="account-container">
		<%- include("../../components/sidebar_account") %>
		<section class="warranty__main">
			<div class="purchase__main__item">
				<% purchaseHistory.forEach(order => { %>
				<% if (order.order_status == 'Hoàn thành') { %>
				<%- include('../../components/warranty-item', { order: order }) %>
				<% } %>
				<% }) %>
			</div>
		</section>
	</main>

	<%- include('../../components/success-modal', { action: 'Gửi yêu cầu bảo hành' }) %>
	<%- include('../../components/scrollup') %>
	<%- include('../../partials/footer') %>
	<%- include('../../partials/app-bar') %>

	<script src="/scripts/pages/account/warranty-claim.js"></script>
	<script src="/scripts/partials/header.js"></script>

</body>

</html>